<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="XToolbar">
        <attr name="showtype" format="enum">
            <enum name="normal" value="0" />
            <enum name="search" value="1" />
            <enum name="organize" value="2" />
        </attr>
        <attr name="immersive" format="boolean" />
        <attr name="statusBarColor" format="color" />
        <attr name="statusBarAlpha" format="float" />
    </declare-styleable>
</resources>